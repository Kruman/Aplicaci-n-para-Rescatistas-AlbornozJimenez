<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/datos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondop"
    tools:context=".E1">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="419dp"
        android:layout_height="93dp"
        android:background="@android:drawable/editbox_background_normal"
        android:backgroundTint="#8AABBF"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/inicio"
            android:layout_width="140dp"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/inicio" />

        <ImageButton
            android:id="@+id/histo"
            android:layout_width="140dp"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackground"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/historial" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="140dp"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cuenta" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/mueda"
        android:text="Cuenta"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gui"
        android:layout_width="139dp"
        android:layout_height="139dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/logo" />

    <LinearLayout
        android:layout_width="303dp"
        android:layout_height="355dp"
        android:layout_marginTop="120dp"
        android:background="@android:drawable/editbox_background_normal"
        android:backgroundTint="@android:color/system_accent2_300"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <ImageButton
            android:id="@+id/config"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#DDF6C1"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/configuracion" />

        <ImageButton
            android:id="@+id/mensajeria"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#FFDBDB"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/mensajes" />

        <ImageButton
            android:id="@+id/guiaa"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#FFEEB9"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/guia" />

        <ImageButton
            android:id="@+id/informacion"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#DECCFF"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/informacion" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>