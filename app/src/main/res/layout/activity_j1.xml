<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondop"
    tools:context=".J1" >


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="420dp"
        android:layout_height="100dp"
        android:background="@android:drawable/editbox_dropdown_light_frame"
        android:backgroundTint="#8AABBF"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/init"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackground"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/inicio" />

        <ImageButton
            android:id="@+id/historial"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackground"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/historial" />

        <ImageButton
            android:id="@+id/datos"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="?attr/selectableItemBackground"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cuenta" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Configuracion"
        android:fontFamily="@font/mueda"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/regresa"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/regresar" />

    <LinearLayout
        android:layout_width="309dp"
        android:layout_height="352dp"
        android:layout_marginTop="100dp"
        android:background="@android:drawable/editbox_background_normal"
        android:backgroundTint="@android:color/system_accent2_300"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <ImageButton
            android:id="@+id/CamCorreo"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#D5BEFF"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cambiarcorreo" />

        <ImageButton
            android:id="@+id/CambContraseÃ±a"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#FFDAA4"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cambiarcontra" />

        <ImageButton
            android:id="@+id/CambNum"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#FFB8B8"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cambiartelefono" />

        <ImageButton
            android:id="@+id/CerrarSecion"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="@android:drawable/editbox_background_normal"
            android:backgroundTint="#D3FFA0"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/cerrarsesion" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>